		this.tpsInvisible = tpsInvisible;
	}
	public int getTpsvisible() {
		return Tpsvisible;
	}
	public void setTpsvisible(int tpsvisible) {
		Tpsvisible = tpsvisible;
	}
	
	public PersonnageInvisible(String nom,int nbPts,int nbVies, boolean Invisible)
	
	
}
