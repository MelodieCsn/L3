package ClasseGe;

public class main {
	public static void main (String[] args){
		CPile<String> c1= new CPile<String>();
		
		String a = "coucou";
		String b = "bonswar";
		String c = "bonne sieste";
		
		System.out.println("pile vide ? : "+c1.estvide() );
		
		c1.empile(a);
		c1.empile(b);
		c1.empile(c);
		
		System.out.println("il y a "+c1.nbElements()+" elements dans cette pile");
		
		

	}
}
