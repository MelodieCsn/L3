package Expressions;

public class main {
	public static void main(String[] args) {
	Constante a=new Constante (3);
	Constante b=new Constante (4);
	char op = '+';
	ExpressionComplexe e= new ExpressionComplexe(a,op,b);
	//System.out.println(e.eval());
	
	Object o = new Constante(7);
	System.out.println(e.eval().o);
	}
}
