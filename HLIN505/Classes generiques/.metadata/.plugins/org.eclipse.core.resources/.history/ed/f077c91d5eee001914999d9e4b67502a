package ClasseGe;
import java.util.LinkedList;

public class CPile<A> implements IPile<A>{
	
	private LinkedList<A> pile;
	
	boolean estvide(){
		return pile.isEmpty();
	}
	void empile(A e){
		pile.add(e);
	}
	A depile(){
		pile.removeLast();
	}
	int nbElements(){
		return pile.size();
	}
	A sommet(){
		
	}
	

}
